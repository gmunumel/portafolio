<% content_for :head do %>
    <h6><%= link_to 'Home', :root %> â†’ Log in</h6>
<% end %>
<% content_for :menu do %>
    <li><%= link_to 'Home', :root %></li>
    <li><%= link_to 'About', :about_index %></li>
    <li><%= link_to 'Portfolio', :portfolio_index %></li>
    <li><%= link_to 'Blog', :blog_index %></li>
    <li><%= link_to 'Contact', :contact_index %></li>
<% end %>

<h2>Log in</h2>

<div id="contact">
  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="boxes">
        <h6>email<span class="required">*</span></h6>
        <%#= f.label :email %>
        <div class="box">
          <%= f.email_field :email, autofocus: true %>
        </div>

        <h6>password<span class="required">*</span></h6>
        <%#= f.label :password %>
        <div class="box">
        <%= f.password_field :password, autocomplete: "off" %>
        </div>
      </div>

      <%# if devise_mapping.rememberable? >
          <div class="box">
            <%= f.check_box :remember_me
            <%= f.label :remember_me
          </div>
      <% end -%>

      <div class="submitbtn">
        <%= f.submit "Log in", class: "button btncolor" %>
      </div>
  <% end %>
</div>

